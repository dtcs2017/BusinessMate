<div id="custom_search_panel" class="row-fluid">
	<div class="span12 basebox">
		<span>高级搜索</span>
		<a id="removeBtn"> <i class="icon-remove remove"></i>
		</a>
		<div class="search_type search_group">
			<span>类　型:</span>
			<span class="group_right">
				<span class="type_item">
					<input id="search_check_myId" type="checkbox" checked="check">
					<span class="lbl">&nbsp;采购编号</span>
				</span>
				<span class="type_item">
					<input id="search_check_name" type="checkbox" checked="check">
					<span class="lbl">&nbsp;名称</span>
				</span>
				<span class="type_item">
					<input id="search_check_partyA" type="checkbox" checked="check">
					<span class="lbl">&nbsp;甲方</span>
				</span>
				<span class="type_item">
					<input id="search_check_partyB" type="checkbox" checked="check">
					<span class="lbl">&nbsp;乙方</span>
				</span>
				<span class="type_item">
					<input id="search_check_amount" type="checkbox" checked="check">
					<span class="lbl">&nbsp;合同金额</span>
				</span>
				<span class="type_item">
					<input id="search_check_state" type="checkbox" checked="check">
					<span class="lbl">&nbsp;合同状态</span>
				</span>
			</span>
		</div>
		<div class="search_key search_group">
			<span class="keyword_title">关键字:</span>
			<span>
				<input id="search_keyword" class="group_right search_input" type="text"></span>
		</div>
		<div class="row-fluid">
			<button id="search_btn" class="span2 btn btn-info btn-small">搜　索</button>
			<button id="thisYear_btn" class="span2 btn btn-small hidden-480">前一年</button>
			<button id="lastYear_btn" class="span2 btn btn-small hidden-480">后一年</button>
		</div>
	</div>
</div>

<div class="contract-content">

	<div class="widget-box transparent">
		<div class="widget-header">
			<h3 class="smaller lighter green">
				合同事件列表
				<span class="green">展现</span>

				<select id="pageSize" class="mySelect">
					<option value="5">5</option>
					<option value="10" selected="selected">10</option>
					<option value="25">25</option>
					<option value="50">50</option>
					<option value="100">100</option>
				</select>

				<span class="green">条</span>
			</h3>
			<button id="exportContracts" class="btn btn-success btn-small" style="margin-right:-20px;">Excel</button>
			<div class="widget-toolbar no-border span2 pull-right">
				<div class="controls">
					<button data-toggle="dropdown" class="btn btn-success btn-small dropdown-toggle span12">
						添加合同
						<span class="caret"></span>
					</button>

					<ul class="dropdown-menu dropdown-success pull-right">
						<li>
							<a href="/contracts/new">人力外包</a>
						</li>

						<li>
							<a href="/contracts/new">固定价格</a>
						</li>

						<li>
							<a href="/contracts/new">产品采购</a>
						</li>

						<li>
							<a href="/contracts/new">系统集成</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</div>

